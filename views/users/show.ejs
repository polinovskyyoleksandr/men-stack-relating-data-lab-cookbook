<!DOCTYPE html>
<html>
<head>
  <title><%= profileUser.username %>'s Pantry</title>
</head>
<body>
  <%- include('../partials/_navbar.ejs') %>
    <h1><%= profileUser.username %>'s Pantry</h1>
    <% if (profileUser.pantry.length === 0) { %>
    <p>No items yet.</p>
    <% } else { %>
    <ul>
      <% profileUser.pantry.forEach(food => { %>
        <li><%= food.name %></li>
      <% }) %>
    </ul>
  <% } %>
</body>
</html>
